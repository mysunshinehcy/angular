{
    "version": "0.2.0",
    "configurations": [
      
      {
        "name": "ng serve",
        "type": "chrome",
        "request": "launch",
        "url": "http://192.168.0.100:8000/chromeapp/tab.html#/",
        "webRoot": "${workspaceRoot}"
      },
      {
        "name": "ng test",
        "type": "chrome",
        "request": "launch",
        "url": "http://localhost:9876/debug.html",
        "webRoot": "${workspaceRoot}"
      },
      {
        "name": "ng e2e",
        "type": "node",
        "request": "launch",
        "program": "${workspaceRoot}/node_modules/protractor/bin/protractor",
        "protocol": "inspector",
        "args": ["${workspaceRoot}/protractor.conf.js"]
      }      
    ]
  }